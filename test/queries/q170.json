{
    "title": "query sum example",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "sumSeats"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "sumSeats"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname"
            ],
            "APPLY": [
                {
                    "sumSeats": {
                        "SUM": "rooms_seats"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
    "result":[{"rooms_shortname":"LSC","sumSeats":700},{"rooms_shortname":"OSBO","sumSeats":442},{"rooms_shortname":"HEBB","sumSeats":375}]
}
